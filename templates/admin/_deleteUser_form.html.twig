<form method="post" action="{{ path('user_delete', {'id': user.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ user.id) }}">
    <button class="btn_delete"><img title="Supprimer" src="https://img.icons8.com/external-kmg-design-detailed-outline-kmg-design/64/000000/external-trash-user-interface-kmg-design-detailed-outline-kmg-design.png"/></button>
</form>